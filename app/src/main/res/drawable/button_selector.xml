<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_activated="true">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="#569874"
                android:startColor="#325691"
                android:type="linear"/>

            <corners android:radius="10dp"/>
        </shape>
    </item>


    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="#569874"
                android:startColor="#325691"
                android:type="linear"/>

            <corners android:radius="10dp"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="90"
                android:endColor="#16272f"
                android:startColor="#27425f"
                android:type="linear"/>

            <corners android:radius="10dp"/>
        </shape>
    </item>



</selector>